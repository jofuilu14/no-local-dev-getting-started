<div class="container">

  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Company</th>
    </tr>

    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.firstname %></td>
        <td><%= lead.lastname %></td>
        <td><%= lead.email %></td>
        <td><%= lead.company %></td>
      </tr>
    <% end %>
    <h1>Inscription</h1>
  </table>
  



</div>

  

