<div class="container">

  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>

    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.firstname %></td>
        <td><%= lead.lastname %></td>
        <td><%= lead.email %></td>
      </tr>
    <% end %>
  </table>
 
</div>
